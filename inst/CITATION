year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "Targeted Learning of a Non-Parametric Variable Importance Measure of a Continuous Exposure",
         author = c(person("Antoine", "Chambaz"),
                    person("Pierre", "Neuvial")),
         year = year,
         note = note,
         url = "http://CRAN.R-project.org/package=tmle.npvi")
